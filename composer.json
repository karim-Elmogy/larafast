{
    "name": "elmogy/larafast",
    "description": "Laravel : Develop Faster And Cleaner!",
    "keywords": [
        "larafast",
        "laravel",
        "module",
        "modules",
        "fast",
        "faster",
        "clean code"
    ],
    "license": "MIT",
    "authors": [
      {
          "name": "Karim Elmogy",
          "email": "kimo72636@gmail.com"
      }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "^6.0",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Elmogy\\Larafast\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Elmogy\\Larafast\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elmogy\\Larafast\\ModuleServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-lv": "vendor/bin/testbench"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
